{
  "name": "hookes-law",
  "version": "1.1.1",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/hookes-law.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "HOOKES_LAW",
    "phetLibs": [
      "twixt"
    ],
    "runnable": true,
    "supportedBrands": [
      "phet"
    ],
    "simFeatures": {
      "colorProfiles": [
        "default"
      ],
      "supportsDynamicLocale": true
    },
    "simulation": true,
    "phet-io": {
      "wrappers": [
        "phet-io-sim-specific/repos/hookes-law/wrappers/hookes-law-energy"
      ]
    },
    "supportsOutputJS": true,
    "published": true,
    "screenNameKeys": [
      "HOOKES_LAW/intro",
      "HOOKES_LAW/systems",
      "HOOKES_LAW/energy"
    ]
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "overrides": [
      {
        "files": [
          "**/*.ts"
        ],
        "rules": {
          "@typescript-eslint/ban-ts-comment": [
            "error",
            {
              "ts-expect-error": true,
              "ts-ignore": true,
              "ts-check": true,
              "ts-nocheck": true
            }
          ]
        }
      }
    ]
  }
}